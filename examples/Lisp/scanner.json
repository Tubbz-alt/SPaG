{
    "language": "Lisp",
    "rules": {
        "boolean": "#t|f",
        "character": "#\\[a-zA-Z0-9]",
        "dot": "\\.",
        "float": "[-+]?[0-9]*\\.?[0-9]+([eE][-+]?[0-9]+)?",
        "int": "0|([-+]?[1-9][0-9]*)",
        "left": "\\(",
        "nil": "\\(\\)",
        "right": "\\)",
        "string": "[a-zA-Z0-9]*",
        "symbol": "[a-zA-Z0-9]*"
    }
}
